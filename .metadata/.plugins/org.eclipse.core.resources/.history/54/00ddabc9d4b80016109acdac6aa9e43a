
public class EuclidianAlgorithim {
	
	public static void main(String[] args) {
		double a = 600851475143;
		int b = 0;
		int gcd = euclidianAlgorithim(a,b);
		System.out.println(gcd);
	}
	
	public static int euclidianAlgorithim(int a, int b) {
		int r = a%b;
		if(r == 0) {
			return b;
		}
		else {
			return euclidianAlgorithim(b, r);
		}
	}

}
